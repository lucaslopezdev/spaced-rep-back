{
  "compilerOptions": {
  "target": "es6",                                  
  "module": "amd",                                        
  "sourceMap": true,                                
  "outDir": "./dist",                                 
  "removeComments": true,                          
  "esModuleInterop": true,                             
  "forceConsistentCasingInFileNames": true,          
  "strict": true,                                      
  "noImplicitAny": true,                               
  "skipLibCheck": true                                 
  },
  "exclude": [
    "node_modules"
  ], 
}
